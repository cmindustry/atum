<!DOCTYPE html>
<html>
<%- partial('_partial/head') %>

<body style="background: <%- theme.background %>">
    <div class="container">
        <%- partial('_partial/header') %>
        <main class="main">
            <%- body %>
        </main>
        <aside class="aside">
            <div class="close"></div>
            <section class="aside-section">
                <%- partial('_widget/category') %>
            </section>
            <section class="aside-section">
                <%- partial('_widget/archive') %>
            </section>
            <section class="aside-section tag">
                <%- partial('_widget/tag') %>
            </section>
            <section class="aside-section instruc">
                <%= config.description %>
            </section>
            <div class="links">
                <ul>
                    <% for (name in theme.social) { %>
                    <li>
                        <a href="<%- url_for(theme.social[name]) %>">
                            <%- name %>
                        </a>
                    </li>
                    <% } %>
                </ul>
            </div>
        </aside>
    </div>
</body>

</html>